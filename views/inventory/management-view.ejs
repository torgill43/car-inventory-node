<!-- Title -->
<% if (title) { %>
    <h1><%= title %></h1>
    <% } else {
      res.redirect('/')
    } %>

<!-- Error -->
<% if (errors) { %>
    <p class="notice"><%= error %></p>
    <% } %>

<!-- Message -->
<% if (message) { %>
    <p class="notice"><%= message %></p>
    <% } %>

<div class="manage-cont">
    <a href="./inv/add-class">Add new classification</a>
    <a href="./inv/add-vehicle">Add new vehicle</a>
</div>